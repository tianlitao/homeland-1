<% title_tag t('pages.wiki_index') %>

<div class="row">
  <%= render "sidebar" %>

  <div class="col-md-9">
    <div class="card card-lg">
      <div class="card-body">
        <div class="card-title hide-ios"><%= t("pages.wiki_index") %></div>
        <ul>
          <% @pages.each do |item| %>
            <li><%= page_title_tag(item) %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
